//uva673

#include<iostream>
#include<stack>
#include<sstream>
#include<cstring>
using namespace std;


int main()
{
    int n;
    cin>>n;
    getchar();
    while(n--)
    {
       string p;
        getline(cin,p);

        stack<char>s;

        bool flag=true;
        for(int i=0;i<p.length();i++)
        {
            if(p[i]=='('||p[i]=='[')
                s.push(p[i]);
            else if(p[i]==')')
            {
                if(s.empty()) flag=false;
                else if(s.top()=='(')
                    s.pop();
                else flag=false;
            }
            else if(p[i]==']')
            {
                if(s.empty()) flag=false;
                else if(s.top()=='[')
                    s.pop();
                else flag=false;
            }
            if(!flag)
            {
                break;
            }
        }

        if(!s.empty()||!flag)cout<<"No"<<endl;
        else
            cout<<"Yes"<<endl;

    }
}
