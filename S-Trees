uva712

#include<stdio.h>
#include<string.h>
#include<math.h>
#define N 1000
#define M 10


int main()
{
    int ca=1,n,m;
    char wev[N],seq[M],ans[M],leave[N];
    while(scanf("%d",&n),n)
    {
        memset(wev,0,sizeof(wev));
        memset(seq,0,sizeof(ans));
        memset(ans,0,sizeof(ans));

        for(int i=0;i<n;i++)
        {
            scanf("%s",wev);
        }

        scanf("%s",leave);
        scanf("%d",&m);

        int idx_lmleave=pow(2,n);
        for(int i=0;i<m;i++)
        {
            scanf("%s",seq);
            int k=1;
            for(int j=0;j<n;j++)
            {

                if(seq[j]=='1')k=k*2+1;
                else k=k*2;
            }
            ans[i]=leave[k-idx_lmleave];

        }
        ans[m]='\0';
        printf("S-Tree #%d:\n%s\n\n",ca++,ans);
    }
    return 0;
}
